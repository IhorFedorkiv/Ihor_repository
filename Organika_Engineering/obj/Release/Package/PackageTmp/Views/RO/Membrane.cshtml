<div id="myModal" class="modal">
    <div class="modal-content">
        <span class="close">&times;</span>
        <h2>FEEDBACK</h2>
        <form action="/RO/email_send" method="post">
            <div class="form-group">
                <label for="name">Name</label>
                <input type="text" name="nameClient" class="form-control" id="name">
            </div>
            <div class="form-group">
                <label for="email">Contacts</label>
                <input type="text" name="emailClient" class="form-control" id="email" placeholder="phone or e-mail">
            </div>
            <div class="textARR">
                <label>Short message</label>
                <textarea name="messageClient" placeholder="Short summary of your question/inquiry" style="height:126px"></textarea>
            </div>
            <button class="blueBTNaa">Send</button>
        </form>
    </div>
</div>
<script>
    var modal = document.getElementById('myModal');
    var btn = document.getElementById("myBtn");
    var span = document.getElementsByClassName("close")[0];
    btn.onclick = function () {
        modal.style.display = "block";
    }
    span.onclick = function () {
        modal.style.display = "none";
    }
    window.onclick = function (event) {
        if (event.target == modal) {
            modal.style.display = "none";
        }
    }
</script>


